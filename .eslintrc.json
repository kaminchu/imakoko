{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "plugins": ["flowtype", "react"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1,
    "quotes": ["error", "double"],
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1
  }
}
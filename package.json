{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "nodemon src/server/app.js --exec babel-node",
    "build:server": "babel src/server -d dist",
    "serve": "node dist/app.js",
    "parcel": "parcel ./client/index.html",
    "build": "parcel build ./src/client/index.html -d public --public-url ./ && cp -rf ./src/client/img ./public/"
  },
  "dependencies": {
    "express": "^4.16.3",
    "material-ui": "^0.20.0",
    "qrcode.react": "^0.8.0",
    "react": "^16.2.0",
    "react-clipboard.js": "^1.1.3",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.17.2",
    "parcel-bundler": "^1.6.2",
    "react-hot-loader": "^4.0.0"
  }
}
